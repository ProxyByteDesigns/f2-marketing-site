{
  "name": "f2",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "fortawesome/react-fontawesome",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "connected-react-router": "^6.4.0",
    "dotenv": "^8.0.0",
    "es6-promise": "^4.2.6",
    "fetch": "^1.1.0",
    "history": "^4.9.0",
    "husky": "^2.2.0",
    "immutable": "^4.0.0-rc.12",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^8.1.6",
    "lodash": "^4.17.11",
    "moment": "^2.29.4",
    "node-sass-chokidar": "^1.3.4",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-alert": "^5.4.0",
    "react-alert-template-basic": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.0.3",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-video": "^1.5.3",
    "reactstrap": "^8.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "universal-cookie": "^3.1.0",
    "video-react": "^0.14.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules  --include-path ./node_modules/bootstrap/scss/ src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules --include-path ./node_modules/bootstrap/scss/ src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettify": "./node_modules/.bin/prettier --single-quote --write 'src/**/*.{js,jsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
